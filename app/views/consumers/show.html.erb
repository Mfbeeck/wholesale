<div id ="consumers_list"
<ul id="Consumers">
	<h1>Consumer: <%= @consumer.username %></h1>
	<div>
	<li>Username: <%= @consumer.username%></li>
	<li>Email: <%= @consumer.email%></li>
	<% if @consumer.phone_number %>
	<li>Phone Number: <%=number_to_phone(@consumer.phone_number)%></li>
	<%end%>

	<%= link_to "Edit", edit_consumer_path(@consumer)%><br>
	<%= link_to "Orders", consumer_orders_path(@consumer)%><br>

	<%#= link_to "Delete User", user_path(@user), method: :delete, data: {confirm: "Are you sure you want to delete the user \"#{@user.email}?\""} %>
	<br>
</div>
<br>
</ul>
