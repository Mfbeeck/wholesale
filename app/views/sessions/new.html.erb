<h1>Log In</h1>
<h1>Suppliers</h1>
<%= form_tag login_supplier_path do %>
  <p>
    <%= label_tag :username %>
    <%= text_field_tag :username %>
  </p>
  <p>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </p>
  <p>
    <%= submit_tag 'Log In' %>
  </p>
<% end %>

<h1>Consumers</h1>
<%= form_tag login_consumer_path do %>
  <p>
    <%= label_tag :username %>
    <%= text_field_tag :username %>
  </p>
  <p>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </p>
  <p>
    <%= submit_tag 'Log In' %>
  </p>
<% end %>
