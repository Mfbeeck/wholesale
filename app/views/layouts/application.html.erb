<!DOCTYPE html>
<html>
<head>
  <title>Wholesale</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body id = "body">

<div id="navbar_container">
	<nav class="navbar">
			<li><%= link_to 'Deals Page (Home)', deals_path %></li>

			<li><% if current_supplier %>
			<%= link_to "Username: #{current_supplier.username}", supplier_path(current_supplier)%>
			&nbsp;&nbsp;
			<%= link_to "Log out", logout_path, method: "delete" %>
			<% else %> 
			<%= link_to 'Supplier Dashboard', new_supplier_session_path %>
			<% end %>
			</li>
			
			<li><% if current_consumer %>
			<%= link_to "Username: #{current_consumer.username}", consumer_path(current_consumer)%>
			&nbsp;&nbsp;
			<%= link_to "Log out", logout_path, method: "delete" %>
			<% else %> 
			<%= link_to 'Consumer Dashboard', new_consumer_session_path %> 
			<% end %>
			</li>
	</nav>
</div>

<div class = "deals_list">
<%= yield %>
</div>

</body>
</html>

